#include<iostream>

using namespace std;

int main()
{

   int num[5];
   int *ptr, i;
   ptr = num;

   cout << "Enter Elements :";
   for(i=0;i<5;i++)
   {
       cin >> num[i];
   }

   cout << "Entered Elements : ";
   for(i=0;i<5;i++)
   {
       cout << *ptr;
       ptr++;
   }

   return 0;
}
